{
  "name": "contentful-app-router",
  "version": "0.1.0",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "setup": "node ./lib/contentful/setup.js",
    "migration:create": "node scripts/create-migration.js",
    "migration:run": "contentful-migration --config contentful-migration-config.js",
    "migration:up": "contentful-migration --config contentful-migration-config.js --yes",
    "migration:dry-run": "contentful-migration --config contentful-migration-config.js --dry-run",
    "migration:check-env": "node scripts/check-env.js",
    "seed:content": "node scripts/seed-content.js",
    "seed:json": "node scripts/seed-from-json.js",
    "seed:clear": "node scripts/clear-content.js",
    "seed:reset": "node scripts/clear-content.js && node scripts/seed-content.js",
    "seed:check": "node scripts/check-env.js && node scripts/seed-content.js",
    "demo:migrate": "node scripts/run-migration.js",
    "demo:seed": "node scripts/seed.js",
    "demo:preview": "contentful-migration --config contentful-migration-config.js --dry-run migrations/001-create-blog-post-type.js",
    "demo:setup": "npm run demo:migrate && npm run demo:seed"
  },
  "dependencies": {
    "@contentful/live-preview": "^2.17.3",
    "@contentful/rich-text-react-renderer": "^15.19.4",
    "contentful": "^10.6.24",
    "dayjs": "^1.11.10",
    "lodash": "^4.17.21",
    "next": "14.1.0",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "contentful-import": "^9.4.38",
    "contentful-management": "^11.54.4",
    "contentful-migration": "^4.29.1",
    "dotenv": "^17.2.1",
    "eslint": "^8",
    "eslint-config-next": "14.1.0",
    "postcss": "^8",
    "tailwindcss": "^3.3.0",
    "typescript": "^5"
  }
}
